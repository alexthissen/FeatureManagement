{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FeatureManagement": {
    "Beta": {
      "EnabledFor": [
        {
          "Name": "AlwaysOn" // Built-in filter name
        }
      ]
    },
    "RetroFonts": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    }
  },
  "AllowedHosts": "*",
  "LeaderboardApiOptions": {
    "BaseUrl": "https://localhost:5002"
  }
}
